<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>

<Pane fx:controller="com.dyakushev.controller.SipTraceController" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextArea fx:id="sipTraceTextArea" prefHeight="366.0" prefWidth="600.0"/>
        <ButtonBar layoutY="366.0" prefHeight="40.0" prefWidth="600.0">
            <buttons>
                <Button mnemonicParsing="false" fx:id="clearButton" onAction="#handleClearAction" text="Clear"/>
                <Button mnemonicParsing="false" fx:id="closeButton" onAction="#handleCloseAction" text="Close"/>
            </buttons>
        </ButtonBar>
    </children>
</Pane>
